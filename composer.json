{
    "name": "your-vendor/laravel-filament-client-upload-cos",
    "description": "Laravel Filament extension for Tencent Cloud COS client-side upload",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "filament",
        "tencent",
        "cos",
        "upload",
        "client-side"
    ],
    "authors": [
        {
            "name": "Your Name",
            "email": "your.email@example.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "filament/filament": "^3.0",
        "illuminate/support": "^10.0|^11.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "YourVendor\\FilamentCosUpload\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "YourVendor\\FilamentCosUpload\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "YourVendor\\FilamentCosUpload\\FilamentCosUploadServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}